<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apurbamondal.loginwithfirebase">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginWithFirebase">
        <activity android:name=".Firebase_storage"></activity>
        <activity android:name=".User_info" />
        <activity android:name=".Profile_activity" />
        <activity android:name=".changeEmail" />
        <activity android:name=".TestActivity" />
        <activity android:name=".password_reset" />
        <activity android:name=".login_activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>